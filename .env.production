NODE_ENV=production
PORT=3003

# Configuración de Puppeteer para Railway
PUPPETEER_EXECUTABLE_PATH=/usr/bin/google-chrome-stable
PUPPETEER_ARGS=--no-sandbox,--disable-setuid-sandbox,--disable-dev-shm-usage,--disable-extensions,--disable-gpu

# Límites de memoria para Railway
NODE_OPTIONS=--max-old-space-size=2048
